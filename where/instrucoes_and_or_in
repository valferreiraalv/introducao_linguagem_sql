/*INSTRUCOES AND, OR, IN*/

SELECT * FROM station_data
WHERE year >= 2005 AND year <= 2010 


SELECT * FROM station_data
WHERE MONTH = 3
OR MONTH = 6 
OR MONTH = 9 
OR MONTH = 12 


SELECT * FROM station_data
WHERE MONTH IN (3,6,9,12)


SELECT * FROM station_data
WHERE MONTH NOT IN (3,6,9,12)


SELECT * FROM station_data
WHERE MONTH % 3 = 0